{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "barryvdh/laravel-dompdf": "^3.0",
        "bensampo/laravel-enum": "^6.1",
        "doctrine/dbal": "^3.5",
        "dompdf/dompdf": "*",
        "greenter/htmltopdf": "^5.0",
        "greenter/lite": "^5.0",
        "greenter/report": "^5.0",
        "greenter/xml": "^5.0",
        "greenter/xmldsig": "^5.0",
        "guzzlehttp/guzzle": "^7.4",
        "intervention/image": "*",
        "jhamnerx/laravel-id-generator": "dev-master",
        "laravel-lang/common": "^6.4",
        "laravel/framework": "^11.0",
        "laravel/jetstream": "^5.0",
        "laravel/sanctum": "^4.0",
        "laravel/tinker": "^2.9",
        "league/flysystem-ziparchive": "^3.24",
        "livewire/livewire": "^3.4",
        "luecano/numero-a-letras": "3.0",
        "maatwebsite/excel": "^3.1",
        "masbug/flysystem-google-drive-ext": "^2.3",
        "netflie/whatsapp-cloud-api": "^2.0",
        "power-components/livewire-powergrid": "^6.1",
        "predis/predis": "^2.2",
        "pusher/pusher-php-server": "^7.2",
        "simplesoftwareio/simple-qrcode": "4.2.0",
        "spatie/laravel-activitylog": "^4.7",
        "spatie/laravel-backup": "^9.2",
        "spatie/laravel-permission": "^6.9",
        "vinkla/hashids": "*",
        "wireui/wireui": "*"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.14",
        "fakerphp/faker": "^1.23",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.1",
        "phpunit/phpunit": "^11.0.1",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
